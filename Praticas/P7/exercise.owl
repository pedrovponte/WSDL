    <!-- http://www.semanticweb.org/pedro/ontologies/2022/10/untitled-ontology-10#Chesterfields -->

    <owl:Thing rdf:about="http://www.semanticweb.org/pedro/ontologies/2022/10/untitled-ontology-10#Chesterfields">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pedro/ontologies/2022/10/untitled-ontology-10#smokes"/>
                    </rdf:Description>
                </owl:onProperty>
